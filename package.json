{
  "name": "walcron-microfrontend-profiler",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:tsc": "tsx copyModuleFederationFiles",
    "build:container": "tsx copyModuleFederationFiles; rm -rf ../container/external_modules/profiler; mkdir ../container/external_modules/profiler; cp -rf dist/* ../container/external_modules/profiler",
    "build:watch": "vite build --watch",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.6",
    "@originjs/vite-plugin-federation": "^1.2.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/google.analytics": "^0.0.42",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-istanbul": "^0.32.4",
    "@yoonghan/walcron-microfrontend-shared": "0.6.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^22.1.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react-router-dom": "^6.13.0",
    "sort-by": "^0.0.2",
    "styled-components": "^5.3.11",
    "tsx": "^3.12.7",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  }
}
